    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.1.0",
    "http-status-codes": "^1.4.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "sequelize": "^6.3.4",
    "tslib": "^2.0.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.7",
    "@types/helmet": "0.0.48",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.6.0",
    "@types/supertest": "^2.0.10",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.2"
