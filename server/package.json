{
  "name": "server",
  "version": "0.0.0",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register ./src --env=production",
    "dev": "nodemon --env=development",
    "test": "nodemon --config ./spec/nodemon.json"
